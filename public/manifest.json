{
    "name": "DDD",
    "version": "1.0",
    "description": "digdigdig.vip一键文章推荐工具",
    "manifest_version": 2,
    "icons": {
      "16": "favicon.ico",
      "32": "favicon.ico",
      "48": "favicon.ico",
      "128": "favicon.ico"
    },
    "browser_action": {
      "default_icon": "favicon.ico",
      "default_popup": "index.html"
    },
    "content_scripts": [
      {
        "matches": [ "<all_urls>" ],
        "js": [ "content-script.js" ]
      }
    ]
}
  